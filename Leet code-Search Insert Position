#include<stdio.h>

    int searchInsert(int num[],int n,int target)
    {
    int low=0;
    int high=n-1;
    int mid=(low+high)/2;
    while(low <= high)
    {
        mid=(low+high)/2;
    if(num[mid]==target)
    return mid;
    else if(num[mid]<target)
    low=mid+1;
    else
    high=mid-1;
    }
    return low;
    }
    
